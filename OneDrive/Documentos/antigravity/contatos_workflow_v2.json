{
    "name": "Supabase Agent - Contatos (Corrigido)",
    "nodes": [
        {
            "parameters": {
                "httpMethod": "POST",
                "path": "contatos",
                "responseMode": "lastNode",
                "options": {}
            },
            "name": "Webhook",
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 1.1,
            "position": [
                250,
                300
            ],
            "webhookId": "supabase-contatos-webhook"
        },
        {
            "parameters": {
                "dataType": "string",
                "value1": "={{ $json.body.operation }}",
                "rules": {
                    "rules": [
                        {
                            "value2": "create"
                        },
                        {
                            "value2": "read"
                        },
                        {
                            "value2": "update"
                        },
                        {
                            "value2": "delete"
                        }
                    ]
                }
            },
            "name": "Router",
            "type": "n8n-nodes-base.switch",
            "typeVersion": 1,
            "position": [
                450,
                300
            ]
        },
        {
            "parameters": {
                "tableId": "contatos",
                "columns": "={{ Object.keys($json.body.data || {}).join(',') }}",
                "additionalFields": {}
            },
            "name": "Supabase Create",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                750,
                100
            ],
            "credentials": {
                "supabaseApi": {
                    "id": "YOUR_CREDENTIAL_ID_HERE",
                    "name": "Supabase Account"
                }
            }
        },
        {
            "parameters": {
                "operation": "getAll",
                "tableId": "contatos",
                "returnAll": true,
                "additionalFields": {}
            },
            "name": "Supabase Read",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                750,
                300
            ],
            "credentials": {
                "supabaseApi": {
                    "id": "YOUR_CREDENTIAL_ID_HERE",
                    "name": "Supabase Account"
                }
            }
        },
        {
            "parameters": {
                "operation": "update",
                "tableId": "contatos",
                "updateKey": "id",
                "columns": "={{ Object.keys($json.body.data || {}).join(',') }}",
                "value": "={{ $json.body.id }}"
            },
            "name": "Supabase Update",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                750,
                500
            ],
            "credentials": {
                "supabaseApi": {
                    "id": "YOUR_CREDENTIAL_ID_HERE",
                    "name": "Supabase Account"
                }
            }
        },
        {
            "parameters": {
                "operation": "delete",
                "tableId": "contatos",
                "deleteKey": "id",
                "value": "={{ $json.body.id }}"
            },
            "name": "Supabase Delete",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                750,
                700
            ],
            "credentials": {
                "supabaseApi": {
                    "id": "YOUR_CREDENTIAL_ID_HERE",
                    "name": "Supabase Account"
                }
            }
        },
        {
            "parameters": {
                "jsCode": "// Preparar resposta de sucesso\nconst operation = $('Webhook').first().json.body.operation;\nconst data = $input.all();\n\nreturn [\n  {\n    json: {\n      success: true,\n      operation: operation,\n      data: data.map(item => item.json)\n    }\n  }\n];"
            },
            "name": "Prepare Response",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                950,
                300
            ]
        },
        {
            "parameters": {
                "tableId": "logs_agente",
                "columns": "operation, status, details, timestamp",
                "additionalFields": {}
            },
            "name": "Log Success",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1150,
                300
            ],
            "credentials": {
                "supabaseApi": {
                    "id": "YOUR_CREDENTIAL_ID_HERE",
                    "name": "Supabase Account"
                }
            }
        },
        {
            "parameters": {
                "jsCode": "const operation = $('Webhook').first().json.body.operation;\nconst status = 'success';\nconst timestamp = new Date().toISOString();\nconst details = JSON.stringify($('Prepare Response').first().json);\n\nreturn [\n  {\n    json: {\n      operation,\n      status,\n      timestamp,\n      details\n    }\n  }\n];"
            },
            "name": "Prepare Log",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                1150,
                150
            ]
        }
    ],
    "connections": {
        "Webhook": {
            "main": [
                [
                    {
                        "node": "Router",
                        "type": "main",
                        "index": 0
                    }
                ]
